{
  "name": "sample-cf-wasm",
  "version": "1.0.0",
  "description": "Cloudflare WebAssembly Worker Demo",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:wasm && npm run build:worker",
    "build:wasm": "cd wasm && wasm-pack build --target web --out-dir ../src/wasm-pkg",
    "build:worker": "wrangler build",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "preview": "wrangler preview"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "keywords": ["cloudflare", "worker", "webassembly", "wasm"],
  "author": "",
  "license": "MIT"
} 